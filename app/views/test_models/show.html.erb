<div class="page-header">
  <%= link_to test_models_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_test_model_path(@test_model), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show test_model</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @test_model.title %></dd>

  <dt>Body:</dt>
  <dd><%= @test_model.body %></dd>

  <dt>Published:</dt>
  <dd><%= @test_model.published %></dd>

  <dt>Publish at:</dt>
  <dd><%= @test_model.publish_at %></dd>

  <dt>Conversion rate:</dt>
  <dd><%= @test_model.conversion_rate %></dd>

  <dt>Views counter:</dt>
  <dd><%= @test_model.views_counter %></dd>

  <dt>State:</dt>
  <dd><%= @test_model.state_id %></dd>

  <dt>City:</dt>
  <dd><%= @test_model.city_id %></dd>

</dl>
